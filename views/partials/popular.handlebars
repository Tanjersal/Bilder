<!--Partials layout to display wherever in ap-->

<div class="panel panel-primary">
    <div class="panel-heading">
        <h3 class="panel-title">Most Popular</h3>
    </div>
    <div class="panel-body">
        {{#each sidebar.popular}}
        <div class="col-md-4 text-center" style="padding-bottom: .5empx;">
            <a href="/images/{{uniqueID}}">
                <img src="/public/upload/{{filename}}" style="height: 75px; width:75px;" class="img-thumbnail">
            </a>
        </div>
        {{/each}}
    </div>
</div>